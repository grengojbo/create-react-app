{
  "name": "react-error-overlay",
  "version": "4.0.0-1",
  "description": "An overlay for displaying stack frames.",
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run build:prod && npm test",
    "start": "cross-env NODE_ENV=development node build.js --watch",
    "test": "cross-env NODE_ENV=test jest",
    "build": "cross-env NODE_ENV=development node build.js",
    "build:prod": "cross-env NODE_ENV=production node build.js"
  },
  "repository": "grengojbo/es6-react-scripts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grengojbo/es6-react-scripts/issues"
  },
  "keywords": [
    "overlay",
    "syntax",
    "error",
    "red",
    "box",
    "redbox",
    "crash",
    "warning"
  ],
  "author": "Joe Haddad <timer150@gmail.com>",
  "files": [
    "lib/index.js"
  ],
  "devDependencies": {
    "anser": "1.4.4",
    "babel-code-frame": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.1.0",
    "chokidar": "^1.7.0",
    "cross-env": "5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.73.0",
    "html-entities": "1.2.1",
    "jest": "^23.1.0",
    "jest-fetch-mock": "1.6.2",
    "object-assign": "4.1.1",
    "promise": "^8.0.1",
    "raw-loader": "^0.5.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "rimraf": "^2.6.2",
    "settle-promise": "1.0.0",
    "source-map": "^0.7.3",
    "webpack": "^3.12.0"
  },
  "jest": {
    "setupFiles": [
      "./src/__tests__/setupJest.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "setupJest.js"
    ]
  }
}
